	<!-- 门诊住院 -->
	<dubbo:reference id="medicalApiServer"
		interface="net.jqsoft.ncmsapi.core.medical.api.rpc.MedicalRpcService" ></dubbo:reference>
	<dubbo:reference id="medicalFeeApiServer"
		interface="net.jqsoft.ncmsapi.core.medical.api.rpc.MedicalFeeRpcService" ></dubbo:reference>
	<dubbo:reference id="compenApiServer"
					 interface="net.jqsoft.ncmsapi.core.compen.api.rpc.CompenRpcService" ></dubbo:reference>
	<!-- 门诊住院 -->
	<!-- ================ 门诊住院模块 =========================== -->
