<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <!--知识库 -->
    <package name="repository" namespace="/admin/repository" extends="ncms-default">     
        <!-- 知识库种类 -->
        <action name="repositoryKind" class="net.jqsoft.repository.action.RepositoryKindAction">
        	<result name="success" type="freemarker">/WEB-INF/pages/repository/repositoryKind_list.html</result>
        	<result name="input" type="freemarker">/WEB-INF/pages/repository/repositoryKind_edit.html</result>
        	<result name="proKindList"  type="json"><param name="root">proKindList</param></result>
        	<result name="RepoKind" type="freemarker">/WEB-INF/pages/repository/repositoryKind_view.html</result>
        
        </action>
        <!-- 知识库基本信息 -->
        <action name="repositoryBase" class="net.jqsoft.repository.action.RepositoryBaseAction">
        <result name="success" type="freemarker">/WEB-INF/pages/repository/repositoryBase_list.html</result>
        <result name="input" type="freemarker">/WEB-INF/pages/repository/repositoryBase_edit.html</result>
       <result name="sonList"  type="freemarker">/WEB-INF/pages/repository/repositoryBase_sonList.html</result>
        <result name="RepoBase" type="freemarker">/WEB-INF/pages/repository/repositoryBase_view.html</result>
        <result name="viewHots" type="freemarker">/WEB-INF/pages/repository/repositoryBase_viewHots.html</result>
        
        </action>
        	
    </package>
</struts>
