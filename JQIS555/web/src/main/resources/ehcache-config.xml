           <!--eternal="false"-->
           <!--timeToLiveSeconds="10"-->
           <!--timeToIdleSeconds="20"-->
           <!--diskPersistent="false" />-->
    <cache
        name="SysOrg.sysOrgCache"
        maxElementsInMemory="10000"
        eternal="false"
        maxElementsOnDisk="1000"
        overflowToDisk="true"
        timeToIdleSeconds="300" />

</ehcache>