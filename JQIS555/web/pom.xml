	<!-- 插件配置 -->
	<build>
		<finalName>jqis-web</finalName>
		<!-- 命令行输入mvn时，默认执行mvn package -->
		<defaultGoal>jetty:run</defaultGoal>
		<!-- 设定Web应用中编译结果输出到WEB-INF/classes �? 与使用IDE时相�? -->
		<resources><!-- 存放资源文件。这里的文件不需要编译，在部署打包时，直接复制这里的文件 -->
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
				<excludes>
					<exclude>**/*.java</exclude>
					<exclude>test/*</exclude>
					<exclude>production/*</exclude>
					<exclude>development/*</exclude>
					<!--例外的文件类型。如果这里存放有.java文件，则仍然需要编译�? -->
				</excludes>
				<!-- 资源根目录排除各环境的配置，使用单独的资源目录来指定 -->
			</resource>
			<resource>
				<directory>src/main/resources/${profiles.active}</directory>
			</resource>
			<resource>
				<directory>src/main/java</directory>
				<!-- -->
				<excludes>
					<exclude>**/*.java</exclude>
					<!--例外的文件类型。如果这里存放有.java文件，则仍然需要编译�? -->
				</excludes>
			</resource>
		</resources>

		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>exec-maven-plugin</artifactId>
				<version>1.2.1</version>

				<configuration>
					<executable>java</executable>
					<arguments>
						<!--<argument>-Djava.ext.dirs=D:\jinqi\hecv\trunk\target\hecv\WEB-INF\lib</argument> 
							&lt;!&ndash; 这是一个系统属性参数 &ndash;&gt; -->
						<argument>-XX:MaxPermSize=256m</argument>
						<!-- 这是一个JVM参数 -->
						<!--<argument> -Djava.ext.dirs=D:\jinqi\hecv\trunk\target\hecv\WEB-INF\lib</argument> -->
						<argument>-classpath</argument>
						<!-- 这是classpath属性，其值就是下面的<classpath/> -->
						<classpath />
						<!-- 这是exec插件最有价值的地方，关于工程的classpath并不需要手动指定，它将由exec自动计算得出 -->
						<argument>org.zcframework.generator.Generator</argument>
						<!-- 程序入口，主类名称 -->
					</arguments>
				</configuration>

			</plugin>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>2.2</version>
				<configuration>
					<contextReloadable>false</contextReloadable>
					<warSourceDirectory>${basedir}/webapp</warSourceDirectory>
					<uriEncoding>UTF-8</uriEncoding>
					<path>/${project.artifactId}</path>
					<url>${deploy.url}</url>
					<username>admin</username>
					<password>1</password>
					<port>8085</port>
				</configuration>
			</plugin>

		</plugins>
	</build>
